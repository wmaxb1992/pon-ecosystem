# Multi-Worker AI Configuration
# =============================

# Grok AI Configuration
GROK_API_KEY=your_grok_api_key_here
GROK_MODEL=grok-beta
GROK_MAX_TOKENS=4000
GROK_TEMPERATURE=0.7

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB_BROKER=0
REDIS_DB_RESULT=0
REDIS_DB_MEMORY=1

# Celery Configuration
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0
CELERY_TASK_SERIALIZER=json
CELERY_ACCEPT_CONTENT=json
CELERY_RESULT_SERIALIZER=json

# Worker Configuration
CODE_WORKER_CONCURRENCY=2
QUALITY_WORKER_CONCURRENCY=1
MEMORY_WORKER_CONCURRENCY=1
WORKER_PREFETCH_MULTIPLIER=1
WORKER_MAX_TASKS_PER_CHILD=1000

# Queue Configuration
CODE_QUEUE_NAME=code_queue
QUALITY_QUEUE_NAME=quality_queue
MEMORY_QUEUE_NAME=memory_queue

# Task Timeouts (seconds)
CODE_TASK_TIMEOUT=300
QUALITY_TASK_TIMEOUT=180
MEMORY_TASK_TIMEOUT=120
DEFAULT_TASK_TIMEOUT=600

# File Operations
MAX_FILE_SIZE_MB=10
ALLOWED_FILE_EXTENSIONS=.py,.js,.ts,.html,.css,.json,.md,.txt,.yaml,.yml
BACKUP_ENABLED=true
BACKUP_DIR=backups

# Quality Assurance
QA_MIN_SCORE=70
QA_CRITICAL_ISSUES_BLOCK=true
QA_AUTO_FIX_ENABLED=true

# Memory & Indexing
MEMORY_MAX_ENTRIES=10000
MEMORY_CLEANUP_INTERVAL=3600
INDEX_UPDATE_INTERVAL=300
PATTERN_LEARNING_ENABLED=true

# Monitoring
ENABLE_MONITORING=true
MONITORING_PORT=5555
LOG_LEVEL=INFO
METRICS_RETENTION_DAYS=30

# Security
ENABLE_SECURITY_CHECKS=true
SCAN_FOR_SECRETS=true
VALIDATE_IMPORTS=true

# Performance
ENABLE_CACHING=true
CACHE_TTL=3600
MAX_CONCURRENT_TASKS=10

# Development
DEBUG_MODE=false
VERBOSE_LOGGING=false
SAVE_INTERMEDIATE_RESULTS=false
